    "compression": "1.6.2",
    "cors": "2.7.1",
    "crypto-js": "3.1.6",
    "feathers": "2.0.1",
    "feathers-authentication": "0.7.9",
    "feathers-blob": "1.1.0",
    "feathers-configuration": "0.3.0",
    "feathers-errors": "2.4.0",
    "feathers-hooks": "1.5.4",
    "feathers-mailgun": "0.1.2",
    "feathers-memory": "0.7.5",
    "feathers-populate-hook": "0.0.4",
