{
  "name": "@doubleqliq/wb-backend",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-server",
    "start-beanstalk": "npm run build && node ./build/index.js",
    "start": "NODE_ENV=development node ./build/index.js",
    "watch": "NODE_ENV=development nodemon --exec babel-node src",
    "fixtures": "NODE_ENV=development babel-node fixtures",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "aws-sdk": "2.4.11",
